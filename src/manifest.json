{
    "manifest_version": 3,
    "name": "Simkl>Plex",
    "description": "Sync Simkl watch history to your plex account",
    "homepage_url": "https://github.com/SIMKL/Sync-Simkl-to-Plex-Chrome-Extension",
    "version": "0.0.1",
    "icons": {
        "16": "assets/icon-16x16.png",
        "48": "assets/icon-48x48.png",
        "128": "assets/icon-128x128.png",
        "512": "assets/icon-512x512.png"
    },
    "background": {
        "service_worker": "/js/background/background.js"
    },
    "key": "",
    "action": {
        "default_title": "Simkl>Plex"
    },
    "host_permissions": [
        "http://edjlcleicmcdpapdcobooenkchaehdib/*",
        "https://api.simkl.com/*",
        "https://*.plex.tv/*"
    ],
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "ruleset_1",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    },
    "web_accessible_resources": [
        {
            "resources": [
                "popup.html"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "permissions": [
        "declarativeNetRequest",
        "contextMenus",
        "webRequest",
        "alarms",
        "storage"
    ]
}